local advantageSec = tabGame:CreateSection("Advantage ESP")

local advantageToggle = tabGame:CreateToggle({
    Name = "Advantage ESP",
    CurrentValue = false,
    Callback = function(v)
        for i,v in pairs(workspace:GetDescendants()) do
            if v:IsA("TouchTransmitter") and v.Parent:IsA("MeshPart") and v.Parent.Parent:IsA("Model") and v.Parent.Parent.Parent:IsA("MeshPart") and v.Parent.Parent.Parent.Parent:IsA("Model") then
                local esp = Drawing.new("Text")
                esp.Visible = false;
                esp.Size = 13;
                esp.Visible = v;
                esp.Position = Vector2.new(v.Position.X,v.Position.Y)

            end
        end
    end,

    
})
